#: modules/access/dshow/dshow.cpp:83 modules/codec/avcodec/avcodec.c:71
#: modules/gui/qt4/components/interface_widgets.cpp:279
#: modules/video_output/opengl.c:126 modules/video_output/opengl.c:182
